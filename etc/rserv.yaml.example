server:
  addr: upstream.irc.net
  port: 6697
  ssl: yes
  reconnectdelay: 30
link: 
  name: rserv.irc.net
  serverid: 0RS
  description: Ruby Services
  protocol: ts6
  sendpassword: password-here
  recvpassword: password-here
channels:
  - opers
plugins:
  - none  
control:
  name: RServ
  user: rserv
  host: link #Setting this to "link" will make it the same as link.name above. Otherwise it will make it the given value.
  gecos: Ruby Services
  prefix: '@' #Command prefix